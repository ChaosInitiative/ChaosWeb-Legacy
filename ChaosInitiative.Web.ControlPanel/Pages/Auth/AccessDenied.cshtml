@page
@{
    Layout = "Shared/_LayoutPaper";
    ViewData["Title"] = "Access Denied";
}
<h1>Access denied</h1>

<p>Access to this page is denied.</p>

@if(User.Identity.IsAuthenticated){
    <p>You are authenticated as <strong>@User.Identity.Name</strong></p>
}
else
{
    <p>You are <strong>not</strong> authenticated</p>
}
 