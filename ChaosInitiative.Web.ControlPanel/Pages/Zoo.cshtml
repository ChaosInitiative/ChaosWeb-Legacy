@page
@{
    Layout = "Shared/_LayoutBare";
}
<div>
    <div class="row pt-3">
        <div class="col-2 pl-1 pr-1">
            <a class="btn btn-block btn-primary">Primary</a>
            <a class="btn btn-block btn-secondary">Secondary</a>
            <a class="btn btn-block btn-light">Light</a>
            <a class="btn btn-block btn-dark">Dark</a>
            <a class="btn btn-block btn-success">Success</a>
            <a class="btn btn-block btn-warning">Warning</a>
            <a class="btn btn-block btn-danger">Danger</a>
            <a class="btn btn-block btn-info">Info</a>
            <a class="btn btn-block btn-link">Link</a>
        </div>
        <div class="col-2 pl-1 pr-1">
            <a class="btn btn-block disabled btn-primary">Primary</a>
            <a class="btn btn-block disabled btn-secondary">Secondary</a>
            <a class="btn btn-block disabled btn-light">Light</a>
            <a class="btn btn-block disabled btn-dark">Dark</a>
            <a class="btn btn-block disabled btn-success">Success</a>
            <a class="btn btn-block disabled btn-warning">Warning</a>
            <a class="btn btn-block disabled btn-danger">Danger</a>
            <a class="btn btn-block disabled btn-info">Info</a>
            <a class="btn btn-block disabled btn-link">Link</a>
        </div>
        <div class="col pl-1 pr-1">
            <table class="table table-hover">
                <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Department</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <td>0</td>
                    <td>CitadelCore</td>
                    <td>Project Founder, Sysadmin, Engine Head</td>
                </tr>
                <tr>
                    <td>1</td>
                    <td>Tewan</td>
                    <td>PR, Web Dev, P2CE Game Head</td>
                </tr>
                </tbody>
            </table>
        
            <hr>
        
            <div class="row no-gutters">
                <div class="col no-gutters">
                
                    <div class="alert alert-success">
                        <span class="alert-icon"></span>
                        <p>Success Alert with a <a href="#">link</a></p>
                    </div>

                    <div class="alert alert-warning">
                        <span class="alert-icon"></span>
                        <p>Warning Alert with a <a href="#">link</a></p>
                    </div>

                    <div class="alert alert-danger">
                        <span class="alert-icon"></span>
                        <p>Danger Alert with a <a href="#">link</a></p>
                    </div>

                    <div class="alert alert-info">
                        <span class="alert-icon"></span>
                        <p>Info Alert with a <a href="#">link</a></p>
                    </div>

                </div>
                <div class="col">
                
                    <div class="alert alert-sm alert-success">
                        <span class="alert-icon"></span>
                        <p>Success Alert with a <a href="#">link</a></p>
                    </div>

                    <div class="alert alert-sm alert-warning">
                        <span class="alert-icon"></span>
                        <p>Warning Alert with a <a href="#">link</a></p>
                    </div>

                    <div class="alert alert-sm alert-danger">
                        <span class="alert-icon"></span>
                        <p>Danger Alert with a <a href="#">link</a></p>
                    </div>

                    <div class="alert alert-sm alert-info">
                        <span class="alert-icon"></span>
                        <p>Info Alert with a <a href="#">link</a></p>
                    </div>
                
                </div>
            </div>
        
        </div>
    </div>
    <div class="row">
        <hr>
        <div class="col-2">
            <input type="text" class="form-control" placeholder="Text Box"/>
            <hr>
            <textarea class="form-control" placeholder="Text Area"></textarea>
        </div>
        <div class="col-2">
            <input type="text" class="form-control" placeholder="Disabled Text Box" disabled/>
            <hr>
            <textarea class="form-control" disabled>Disabled Text Area, but this one has text in it and isn't just placeholder text</textarea>
        </div>
        <div class="col">
            <select class="custom-select">
                <optgroup label="Section A">
                    <option>Item A1</option>
                    <option>Item A2</option>
                    <option>Item A3</option>
                </optgroup>
                <optgroup label="Section B">
                    <option>Item B1</option>
                    <option>Item B2</option>
                    <option>Item B3</option>
                </optgroup>
            </select>
            
            <hr/>
            
            <div class="custom-control custom-checkbox">
                <input type="checkbox" id="check1" class="custom-control-input" />
                <label class="custom-control-label" for="check1">Checkbox</label>
            </div>
            
            <div class="custom-control custom-control-inline custom-checkbox">
                <input type="checkbox" disabled id="check2" class="custom-control-input" />
                <label class="custom-control-label" for="check2">Disabled Checkbox</label>
            </div>
            
            <div class="custom-control custom-control-inline custom-checkbox">
                <input type="checkbox" disabled checked id="check3" class="custom-control-input" />
                <label class="custom-control-label" for="check3">Disabled Checkbox</label>
            </div>
            
            <div class="progress mt-1 mb-1">
              <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <div class="progress mt-1 mb-1">
              <div class="progress-bar w-50" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">50%</div>
            </div>
            <div class="progress mt-1 mb-1">
              <div class="progress-bar w-75 progress-bar-striped progress-bar-animated bg-info" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
        </div>
        <div class="col">
            <div class="custom-control custom-switch">
                <input type="checkbox" class="custom-control-input" id="customSwitch1">
                <label class="custom-control-label" for="customSwitch1">A Switch</label>
            </div>
            <div class="custom-control custom-control-inline custom-switch">
                <input type="checkbox" class="custom-control-input" disabled id="customSwitch2">
                <label class="custom-control-label" for="customSwitch2">Disabled Switch</label>
            </div>
            <div class="custom-control custom-control-inline custom-switch">
                <input type="checkbox" class="custom-control-input" disabled id="customSwitch2" checked>
                <label class="custom-control-label" for="customSwitch2">Disabled Switch but active!</label>
            </div>
            
            <hr/>
            
            <div class="list-group">
              <a href="#" class="list-group-item list-group-item-action active">
                Active Element
              </a>
              <a href="#" class="list-group-item list-group-item-action">Action elements</a>
              <a href="#" class="list-group-item list-group-item-action">Action elements</a>
              <a href="#" class="list-group-item list-group-item-action">Action elements</a>
              <a href="#" class="list-group-item list-group-item-action disabled" tabindex="-1" aria-disabled="true">Disabled action element</a>
            </div>
        </div>
    </div>
</div>
<div>
    <section>
        <h1>Section 1</h1>
    </section>
    <section>
        <h1>Section 2</h1>
    </section>
    <section>
        <h1>Section 3</h1>
    </section>
    
    <div class="jumbotron">
        <h1>Jumbotron</h1>
        <p>Has some text</p>
        <hr/>
        <button class="btn btn-secondary">Can also add buttons and stuff</button>
    </div>
</div>

<div>
    <div class="row">
        <div class="col">
            
            <ul class="nav flex-column nav-pills">
                <li class="nav-item">
                    <a class="nav-link active" href="#" aria-current="page">Active</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Link</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Link</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
                </li>
            </ul>
            
        </div>
    </div>
</div>